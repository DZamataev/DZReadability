FRAMEWORK_SEARCH_PATHS = "$(SDKROOT)/Developer/Library/Frameworks" $(inherited) "$(DEVELOPER_FRAMEWORKS_DIR)" "$(DEVELOPER_DIR)/Platforms/iPhoneSimulator.platform/Developer/Library/Frameworks"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/DZReadability" "${PODS_ROOT}/Headers/Kiwi" $(SDKROOT)/usr/include/libxml2
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/DZReadability" -isystem "${PODS_ROOT}/Headers/Kiwi"
OTHER_LDFLAGS = -ObjC -lxml2 -framework XCTest
PODS_ROOT = ${SRCROOT}/Pods