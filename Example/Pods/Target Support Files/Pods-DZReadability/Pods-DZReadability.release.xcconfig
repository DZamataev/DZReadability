GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/DZReadability" "${PODS_ROOT}/Headers/Public/Kiwi" $(SDKROOT)/usr/include/libxml2
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/DZReadability" -isystem "${PODS_ROOT}/Headers/Public/Kiwi"
OTHER_LDFLAGS = -ObjC -l"Pods-DZReadability-DZReadability" -l"xml2"
PODS_ROOT = ${SRCROOT}/Pods